{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "rootDir": "src/server",
    "outDir": "dist",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "lib": ["esnext"],
    "types": ["node"],
    "moduleDetection": "force",
    "noUncheckedSideEffectImports": true
  },
  "include": ["src/server"]
}
